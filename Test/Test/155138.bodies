class CI_Benchmark
!!!338562.php!!!	mark(in name : string) : void
		$this->marker[$name] = microtime(TRUE);
!!!338690.php!!!	elapsed_time(in point1 : string = '', in point2 : string = '', in decimals : int = 4) : string
		if ($point1 === '')
		{
			return '{elapsed_time}';
		}

		if ( ! isset($this->marker[$point1]))
		{
			return '';
		}

		if ( ! isset($this->marker[$point2]))
		{
			$this->marker[$point2] = microtime(TRUE);
		}

		return number_format($this->marker[$point2] - $this->marker[$point1], $decimals);
!!!338818.php!!!	memory_usage() : string
		return '{memory_usage}';
