class CI_DB_sqlite_result
!!!429186.php!!!	num_rows() : int
		return is_int($this->num_rows)
			? $this->num_rows
			: $this->num_rows = @sqlite_num_rows($this->result_id);
!!!429314.php!!!	num_fields() : int
		return @sqlite_num_fields($this->result_id);
!!!429442.php!!!	list_fields() : array
		$field_names = array();
		for ($i = 0, $c = $this->num_fields(); $i < $c; $i++)
		{
			$field_names[$i] = sqlite_field_name($this->result_id, $i);
		}

		return $field_names;
!!!429570.php!!!	field_data() : array
		$retval = array();
		for ($i = 0, $c = $this->num_fields(); $i < $c; $i++)
		{
			$retval[$i]			= new stdClass();
			$retval[$i]->name		= sqlite_field_name($this->result_id, $i);
			$retval[$i]->type		= NULL;
			$retval[$i]->max_length		= NULL;
		}

		return $retval;
!!!429698.php!!!	data_seek(in n : int = 0) : bool
		return sqlite_seek($this->result_id, $n);
!!!429826.php!!!	_fetch_assoc() : array
		return sqlite_fetch_array($this->result_id);
!!!429954.php!!!	_fetch_object(in class_name : string = 'stdClass') : object
		return sqlite_fetch_object($this->result_id, $class_name);
