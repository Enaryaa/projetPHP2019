      operation 226690 "index"
	public explicit_return_type ""
	nparams 0
	
	
	preserve_php_body_indent php_def "  ${comment}${final}${visibility}${abstract}${static}function ${name}${(}${)}${type}
{
  ${body}}
"
	
	
	comment "pour créer une session $this-session->set_userdata(name, array)
pour vérifier on fait $this->session->has_userdata(name)
pour récupérer $this->session->userdata(name)
pour supprimer la session on fait $this->session->unset_userdata(name)
"
      end

